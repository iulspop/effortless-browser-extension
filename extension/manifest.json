{
  "name": "Undistractable",
  "description": "Augment your cognitive control abilities",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [ "webNavigation", "scripting", "storage" ],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [
    {
      "resources": [ "/fonts/*" ],
      "matches": [ "<all_urls>" ]
    }
  ]
}